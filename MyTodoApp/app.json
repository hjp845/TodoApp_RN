{
  "expo": {
    "name": "MyTodoApp",
    "slug": "MyTodoApp",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "me.hanbin.diary",
      "icon": "./assets/diary_icon.png",
      "adaptiveIcon": {
        "backgroundColor" : "#FAE3EB"
      }
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
